{
    "name": "akeneo/marketplace",
    "description": "Akeneo Marketplace based on Drupal 8.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Lonny Loquesol",
            "email": "lonny.loquesol@makina-corpus.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/previousnext/drush_cmi_tools.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "1.6.2",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/big_pipe_sessionless": "1.1",
        "drupal/core": "8.5.1",
        "drupal/ctools": "3.0",
        "drupal/diff": "1.0-rc1",
        "drupal/inline_entity_form": "1.0-beta1",
        "roave/security-advisories": "dev-master",
        "webflo/drupal-finder": "^1.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4",
        "drupal/paragraphs": "1.2",
        "drupal/honeypot": "1.27",
        "drupal/metatag": "1.5",
        "drupal/schema_metatag": "1.2",
        "drupal/pathauto": "1.1",
        "drupal/redirect": "1.1",
        "drupal/sitemap": "1.3",
        "drupal/simple_sitemap": "2.11",
        "drupal/entity_browser": "2.0-alpha2",
        "drupal/entity_embed": "1.0-beta2",
        "drupal/video_embed_field": "2.0-alpha2",
        "drupal/media_entity_video": "2.0-alpha1",
        "drupal/dropzonejs": "2.0-alpha3",
        "drupal/blazy": "1.0-rc2",
        "drupal/slick": "1.0",
        "drupal/crop": "2.0-rc1",
        "drupal/image_widget_crop": "2.1",
        "drupal/focal_point": "1.0-beta6",
        "drupal/webform": "5.0-rc8",
        "drupal/content_lock": "1.0-alpha8",
        "drupal/link_attributes": "1.2",
        "drupal/role_delegation": "1.0-alpha1",
        "drupal/ultimate_cron": "2.0-alpha3",
        "drupal/linkit": "5.0-beta7",
        "drupal/swiftmailer": "1.0-beta2",
        "drupal/externalauth": "1.1",
        "drupal/ldap": "3.x-dev",
        "drupal/migrate_plus": "4.0-beta3",
        "drupal/migrate_tools": "4.0-beta3",
        "drupal/search_api": "1.7",
        "drupal/facets": "1.0-beta2",
        "drupal/charts": "3.0-alpha8",
        "drupal/eu_cookie_compliance": "1.0",
        "drupal/workflow_participants": "2.3",
        "drupal/content_moderation_notifications": "3.0-rc2",
        "drupal/cdn": "3.2",
        "drupal/editor_file": "1.2",
        "drupal/editor_advanced_link": "1.4",
        "drupal/flood_unblock": "1.5",
        "drupal/workbench_access": "1.0-beta1",
        "drupal/feeds": "3.0-alpha1",
        "drupal/feeds_ex": "1.0-alpha1",
        "drupal/yoast_seo": "1.3",
        "drupal/drush-cmi-tools": "dev-8.x-1.x",
        "drush/drush": "8.1.16"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8",
        "drupal/console": "1.7.0",
        "drupal/drupal-extension": "~3.3.0",
        "behat/behat": "^3.0",
        "drupal/coder": "~8.2.0",
        "guzzlehttp/guzzle" : "^6.0@dev",
        "drupal/devel": "1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "process-timeout": 0
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "www/core": ["type:drupal-core"],
            "www/libraries/{$name}": ["type:drupal-library"],
            "www/modules/{$name}": ["type:drupal-module"],
            "www/profiles/{$name}": ["type:drupal-profile"],
            "www/themes/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/core": {
                "2061377 - Allow image style to be selected in image dialog": "https://www.drupal.org/files/issues/2061377-291.patch",
                "2429699 - Allow views filter to be displayed as a select list": "https://www.drupal.org/files/issues/2429699-92.patch",
                "2788777 - Allow a profile to be installed from existing config": "https://www.drupal.org/files/issues/2788777-80.patch"
            },
            "drupal/ctools": {
                "2475595 - Fix Autosubmit in Views": "https://www.drupal.org/files/issues/2475595-ctools-autocomplete-fix-47.patch"
            },
            "drupal/entity_embed": {
                "2832504 - Send the editor ID to ajax requests for previews": "https://www.drupal.org/files/issues/2832504-2.patch"
            },
            "drupal/metatag": {
                "2883718 - Reenable custom Views admin pages": "https://www.drupal.org/files/issues/2018-03-29/metatag-n2883718-65.patch"
            }
        }
    }
}
